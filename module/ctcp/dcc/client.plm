package ctcp_dcc_client;

$_ = 'ctcp_dcc_client';

sub initialize {
  $CLIENTINFO = 'DCC';
}

sub cpss_dcc {
  local($serverno, $prefix, $cmd, @params) = @_;
  local($flag);
  $flag = 0;
  foreach $cno (&'array($'clientlist)) {
    next unless $'server[$cno] == $serverno;
    $flag = 1;
    &'c_print($cno, $prefix, 'PRIVMSG', $params[0], "\cA" . join(' ', $cmd, $params[1]) . "\cA");
  }
  return () if $flag;
  return ($prefix, $cmd, @params);
}

__END__
